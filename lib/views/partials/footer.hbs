<footer class="footer">
  <div class="container-fluid">
    <nav class="pull-left">
      <ul>
        {{# if social.twitter }}
        <li>
          <a href="https://twitter.com/{{ social.twitter }}">
            Twitter
          </a>
        </li>
        {{/if}}
        {{# if social.facebook }}
        <li>
          <a href="https://facebook.com/{{ social.github }}">
            Facebook
          </a>
        </li>
        {{/if}}
        {{# if social.github }}
        <li>
          <a href="https://github.com/{{ social.github }}">
            GitHub
          </a>
        </li>
        {{/if}}
        <li>
          <a href="https://en.wikipedia.org/wiki/Proof_of_Existence">
            History
          </a>
        </li>
      </ul>
    </nav>
    <div class="copyright pull-right">
      &copy; {{ year }} <a href="https://poex.io">PoEx Co., Ltd</a>
    </div>
  </div>
</footer>
